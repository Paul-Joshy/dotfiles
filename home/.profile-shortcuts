dt=~/Documents/dotfiles
alias dt="cd ~/Documents/dotfiles"
alias rdt="r ~/Documents/dotfiles"
alias ndt="nnn ~/Documents/dotfiles"
alias xdt="open ~/Documents/dotfiles"
function mvdt(){ mv $@ ~/Documents/dotfiles;}
function cpdt(){ cp $@ ~/Documents/dotfiles;}

sc=~/Documents/dotfiles/scripts/
